<main>
    <h2>Are you sure you want to delete <%= deck.name %>?</h2>
    
    <form action="/deletedeck" method="POST">
        <input type="submit" value="Yes">
        <input type="hidden" id="deckId" name="deckId" value="<%= deck._id %>">
    </form>

    <a href="/editdeck?id=<%= deck.externalId %>"><button type="button">No</button></a>
</main>