<main>
    <h1>Standings</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Year</th>
                <th>Archetype</th>
                <th>Colors</th>
                <th>ELO</th>
                <th>Record</th>
            </tr>
        </thead>
        <tbody>
            <% decks.forEach(deck => { %>
                <tr>
                    <td><a href="/deck?id=<%= deck.externalId %>"><%= deck.name %></a></td>
                    <td><%= deck.year %></td>
                    <td><%= deck.archetype %></td>
                    <td><%- deck.colors %></td>
                    <td><%= deck.elo %></td>
                    <td><%= deck.wins %> - <%= deck.losses %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <form action="/" method="GET">
        <input type="submit" value="Back">
    </form>
</main>